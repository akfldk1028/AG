{
  "id": "code_execution",
  "name": {
    "en": "Code Execution",
    "ko": "코드 실행"
  },
  "description": {
    "en": "A coding agent writes code and a code executor agent runs it, enabling iterative development with real-time feedback. The coding agent can fix errors based on execution results.",
    "ko": "코딩 에이전트가 코드를 작성하고 코드 실행 에이전트가 이를 실행하여, 실시간 피드백을 통한 반복적 개발을 가능하게 합니다. 코딩 에이전트는 실행 결과를 바탕으로 오류를 수정할 수 있습니다."
  },
  "diagram": "    ┌─────────────────────────────────────────────────────────────┐\n    │                         USER TASK                           │\n    │                    \"분석할 데이터...\"                          │\n    └─────────────────────────┬───────────────────────────────────┘\n                              │\n                              ▼\n    ┌─────────────────────────────────────────────────────────────┐\n    │                      CODING AGENT                           │\n    │                   코드 작성 전문가                             │\n    │           \"Python 코드를 작성하겠습니다...\"                     │\n    └─────────────────────────┬───────────────────────────────────┘\n                              │ 코드 블록\n                              ▼\n    ┌─────────────────────────────────────────────────────────────┐\n    │                   CODE EXECUTOR AGENT                       │\n    │              Docker/Local 실행 환경                          │\n    │                   실행 결과 반환                              │\n    └─────────────────────────┬───────────────────────────────────┘\n                              │ 결과/오류\n                              ▼\n              ┌───────────────┴───────────────┐\n              │                               │\n         [성공]                            [오류]\n              │                               │\n              ▼                               ▼\n    ┌─────────────────┐           ┌─────────────────┐\n    │   최종 결과       │           │  CODING AGENT   │\n    │   사용자에게      │           │   오류 수정     │\n    └─────────────────┘           └────────┬────────┘\n                                           │\n                                           └──→ (반복)",
  "complexity": "medium",
  "when_to_use": [
    "데이터 분석 및 시각화",
    "수학적 계산 및 검증",
    "자동화 스크립트 생성",
    "프로토타입 개발",
    "코드 테스트 및 디버깅",
    "API 테스트"
  ],
  "when_to_avoid": [
    "보안에 민감한 환경",
    "시스템 리소스 제한이 있을 때",
    "신뢰할 수 없는 코드 실행 위험",
    "실행 환경 구성이 어려울 때"
  ],
  "pros": [
    "실제 코드 실행으로 결과 검증",
    "반복적 디버깅 가능",
    "복잡한 계산 수행",
    "데이터 분석 자동화",
    "시각화 생성 가능"
  ],
  "cons": [
    "보안 위험 존재",
    "실행 환경 필요 (Docker 권장)",
    "시스템 리소스 소비",
    "악의적 코드 실행 가능성",
    "의존성 관리 복잡"
  ],
  "execution_environments": [
    {
      "name": "DockerCommandLineCodeExecutor",
      "description": "Docker 컨테이너에서 안전하게 실행",
      "recommended": true,
      "security": "high"
    },
    {
      "name": "LocalCommandLineCodeExecutor",
      "description": "로컬 환경에서 직접 실행",
      "recommended": false,
      "security": "low"
    },
    {
      "name": "AzureContainerCodeExecutor",
      "description": "Azure Container Instances 사용",
      "recommended": true,
      "security": "high"
    }
  ],
  "example_use_cases": [
    {
      "name": "데이터 분석",
      "agents": ["DataAnalystAgent", "PythonExecutor"],
      "task": "CSV 데이터 분석 및 시각화",
      "output": "분석 결과 + 그래프"
    },
    {
      "name": "수학 문제 해결",
      "agents": ["MathCoderAgent", "PythonExecutor"],
      "task": "복잡한 수학 문제 계산",
      "output": "검증된 계산 결과"
    },
    {
      "name": "웹 스크래핑",
      "agents": ["ScraperAgent", "PythonExecutor"],
      "task": "웹 데이터 수집",
      "output": "정제된 데이터"
    }
  ],
  "autogen_implementation": {
    "provider": "autogen_agentchat.teams.RoundRobinGroupChat",
    "config_template": {
      "participants": [
        {
          "name": "coding_agent",
          "type": "AssistantAgent",
          "system_message": "You are a helpful assistant that solves tasks by writing and executing Python code. Write complete, runnable code blocks.",
          "description": "코드 작성 전문가"
        },
        {
          "name": "code_executor",
          "type": "CodeExecutorAgent",
          "executor": {
            "type": "DockerCommandLineCodeExecutor",
            "work_dir": "./workspace",
            "timeout": 60
          },
          "description": "코드 실행 에이전트"
        }
      ],
      "model_client": { "model": "gpt-4o" },
      "max_turns": 10,
      "termination_condition": {
        "type": "TextMentionTermination",
        "text": "TERMINATE"
      }
    },
    "code_example": "from autogen_agentchat.agents import AssistantAgent, CodeExecutorAgent\nfrom autogen_agentchat.teams import RoundRobinGroupChat\nfrom autogen_ext.code_executors.docker import DockerCommandLineCodeExecutor\n\nexecutor = DockerCommandLineCodeExecutor(work_dir='./workspace')\n\ncoding_agent = AssistantAgent(\n    name='coding_agent',\n    model_client=model_client,\n    system_message='Write Python code to solve tasks.'\n)\n\ncode_executor = CodeExecutorAgent(\n    name='code_executor',\n    code_executor=executor\n)\n\nteam = RoundRobinGroupChat(\n    participants=[coding_agent, code_executor],\n    max_turns=10\n)"
  },
  "supported_languages": [
    {
      "language": "Python",
      "executor": "DockerCommandLineCodeExecutor",
      "common_libraries": ["pandas", "numpy", "matplotlib", "requests"]
    },
    {
      "language": "Bash/Shell",
      "executor": "DockerCommandLineCodeExecutor",
      "common_uses": ["파일 조작", "시스템 명령"]
    }
  ],
